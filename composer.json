{
    "name": "kanojo-db/laravel-popularity",
    "type": "library",
    "description": "A Laravel package to track the popularity of model entries based on recorded visits.",
    "keywords": [
        "kanojo",
        "laravel-popular",
        "Laravel",
        "Popular",
        "Laravel Popular",
        "Popularity",
        "laravel-popularity",
        "Laravel Popularity"
    ],
    "homepage": "https://github.com/kanojo-db/laravel-popularity",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Miguel",
            "homepage": "https://github.com/KanojoDb",
            "role": "Developer"
        },
        {
            "name": "Julien Machiels",
            "homepage": "https://github.com/MrTimscampi",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest": "^1.20",
        "psalm/plugin-laravel": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "KanojoDb\\LaravelPopular\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KanojoDb\\LaravelPopular\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "KanojoDb\\LaravelPopular\\LaravelPopularServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
